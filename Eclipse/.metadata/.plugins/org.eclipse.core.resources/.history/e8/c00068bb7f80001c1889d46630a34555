package arrays;

public class TwoDArrayDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int [][] matrixA = new int[3][4];
		
		matrixA[0][0] = 10;
		matrixA[0][0] = 20;
		matrixA[0][0] = 30;
		matrixA[1][0] = 40;
		matrixA[1][] = 50;
		matrixA[1][2] = 60;

	}

}
